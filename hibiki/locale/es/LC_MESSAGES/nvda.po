# Script description
#: globalPlugins/hibiki/__init__.py:283
msgid "Toggle Hibiki on/off"
msgstr "Activar o desactivar Hibiki"

# Status messages
#: globalPlugins/hibiki/__init__.py:298
msgid "Hibiki enabled"
msgstr "Hibiki activado"

#: globalPlugins/hibiki/__init__.py:302
msgid "Hibiki disabled"
